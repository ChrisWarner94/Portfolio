<a id="top-tag"></a>
<section class="section" id="sectionhome">
  <div class="navbarcontainer" id="navbarcontainer">
    <div class="linkscontainer" id="linkscontainer">
      <ul class="linklist">
        <div class="link" id="linkone">
          <li>
            <i class="fa-solid fa-person-digging icon"></i>
            <a href="#sectionProjects" class="navlinkhover textpaddingleft">Projects</a>
          </li>
        </div>
        <div class="link" id="linktwo">
          <li>
            <i class="fa-solid fa-house icon"></i>
            <a href="#top-tag" class="navlinkhover textpaddingleft">Home </a>
          </li>
        </div>
        <div class="link" id="linkthree">
          <li>
            <i class="fa-solid fa-address-book icon"></i>
            <a href="#sectionContact" class="navlinkhover textpaddingleft">Contact</a>
          </li>
        </div>
      </div>
    </div>
  </div>
  <div class="homepagecontainer">
    <div class="homeleftpanel">
      <div class="homeheading">
        <span id="nameheading" class="sentence"></span>
        <span  id="namecursor"></span>
      </div>
      <div class="homeheading">
        <span id="jobtitleheading" class="sentence"></span>
        <span id="jobtitlecursor"></span>
      </div>
      <div class="image" id="homeimage">
        <%= image_tag ("ProfilePicture.jpg"), :height=> "100%", class: "roundedcorners"%>
      </div>
      <div class="introtext" id="introtext">
      </div>
    </div>
    <div class="homerightpanel">
      <div class="stackpanel" id="stackpanel">
        <% @technologies.each do |tech| %>
          <div class="techcard">
            <div class="techname">
              <%= tech %>
            </div>
          </div>
        <% end  %>
      </div>
    </div>
  </div>
</section>
<section class="section" id="sectionproject">
  <div class="projectstitle">
    <h2 id="projectname">
      Projects
    </h2>
  </div>
  <div class="navcontainer">
    <div class="projectnavigation">
      <div class="navarrow">
        <i class="fa-solid fa-arrow-left navarrow" id="navleftarrow"></i>
      </div>
      <div class="title">
        <h2 >
          Recipeasy
        </h2>
      </div>
      <div class="navarrow">
        <i class="fa-solid fa-arrow-right navarrow" id="navrightarrow"></i>
      </div>
    </div>
  </div>
</section>
<section class="section" id="sectioncontact">
</section>
